{
  "name": "Home Scanner",
  "version": "0.5",
  "description": "Search all homes by coordinates",
  "background": {
    "scripts": [
      "background.js",
      "dist/js/jquery-3.4.1.min.js",
      "dist/js/bootstrap.min.js"
    ],
    "persistent": false
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://*/house-finder.html"],
      "css": ["house-finder.css"],
      "js": ["house-finder.js"]
    },
    {
      "matches": ["*://*.hostcompliance.com/*", "*://*.homeaway.com/*", "*://*.google.com/maps/*"],
      "js": ["vrenhancements.js"],
      "all_frames": true
    },
    {
      "matches": ["*://*/popup.html"],
      "js": ["popup.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/homescanner16.png",
      "32": "images/homescanner32.png",
      "48": "images/homescanner48.png",
      "128": "images/homescanner128.png"
    }
  },
  "icons": {
    "16": "images/homescanner16.png",
    "32": "images/homescanner32.png",
    "48": "images/homescanner48.png",
    "128": "images/homescanner128.png"
  },
  "manifest_version": 2
}
