{
  "name": "Home Scanner",
  "version": "1.0",
  "description": "Search all homes by coordinates",
  "background": {
    "scripts": [
      "/dist/js/background.js",
      "dist/js/jquery-3.4.1.min.js",
      "dist/js/bootstrap.bundle.min.js"
    ],
    "persistent": false
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://www.zillow.com/homescanner/"],
      "css": ["/dist/css/homescanner.css", "/dist/css/bootstrap.min.css"],
      "js": ["/dist/js/homescanner.js", "dist/js/jquery-3.4.1.min.js"]
    },
    {
      "matches": [
        "*://*.hostcompliance.com/*",
        "*://*.homeaway.com/*",
        "*://*.vrbo.com/*",
        "*://*.google.com/maps/*"
      ],
      "js": ["/dist/js/vrenhancements.js"],
      "all_frames": true
    },
    {
      "matches": ["*://*/popup.html"],
      "js": ["/dist/js/popup.js"]
    }
  ],
  "web_accessible_resources": [
    "homescanner.html",
    "/dist/css/homescanner.css",
    "/dist/js/homescanner.js",
    "/dist/css/bootstrap.min.css"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/homescanner16.png",
      "32": "images/homescanner32.png",
      "48": "images/homescanner48.png",
      "128": "images/homescanner128.png"
    }
  },
  "icons": {
    "16": "images/homescanner16.png",
    "32": "images/homescanner32.png",
    "48": "images/homescanner48.png",
    "128": "images/homescanner128.png"
  },
  "manifest_version": 2
}
